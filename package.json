{
  "name": "puck-editor-learning",
  "version": "1.0.0",
  "description": "A simple React project for learning Puck editor",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "node node_modules/vite/bin/vite.js build && node -e \"require('fs').copyFileSync('staticwebapp.config.json', 'build/staticwebapp.config.json')\"",
    "build:analyze": "node node_modules/vite/bin/vite.js build --mode analyze",
    "preview": "node node_modules/vite/bin/vite.js preview",
    "server": "node server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\""
  },
  "dependencies": {
    "@measured/puck": "^0.20.1",
    "@untitled-ui/icons-react": "^0.1.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-select": "^5.8.0",
    "react-time-picker": "^8.0.2",
    "survey-core": "^2.3.7",
    "survey-react-ui": "^2.3.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.8.29",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.14",
    "terser": "^5.44.1",
    "vite": "^5.4.10"
  },
  "keywords": [
    "react",
    "puck",
    "editor",
    "learning"
  ],
  "author": "",
  "license": "MIT"
}
